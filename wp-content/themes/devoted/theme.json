        "custom": {
            "line-height": {
				"body": 1.6,
				"heading": 1.2,
				"heading-large": 1.3
			},
            "status-palette": {
                "error-light": "#EAA6A6",
                "error-base": "#B30F0F",
                "error-dark": "#630202",
                "warning-light": "#F1E1B4",
                "warning-base": "#DEAB14",
                "warning-dark": "#8A6904",
                "success-light": "#A9D1A6",
                "success-base": "#379E2D",
                "success-dark": "#095402"
            }
        },
        "blocks": {
            "core/image": {
                "lightbox": {
                    "allowEditing": true
                }
            }
        }
	},
    "styles": {
		"color": {
			"background": "var(--wp--preset--color--neutral-white)",
			"text": "var(--wp--preset--color--neutral-darkest)"
		},
        "spacing": {
            "blockGap": "var(--wp--preset--spacing--medium)",
            "padding": {
                "left": "var(--wp--preset--font-size--medium)",
                "right": "var(--wp--preset--font-size--medium)"
            }
        },
        "typography": {
			"fontFamily": "var(--wp--preset--font-family--font-primary)",
            "fontSize": "var(--wp--preset--font-size--medium)",
            "lineHeight": "var(--wp--custom--line-height--body)"
		}
    }
