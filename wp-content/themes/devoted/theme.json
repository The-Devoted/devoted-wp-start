{
    "$schema": "https://schemas.wp.org/trunk/theme.json",
    "version": 3,
    "templateParts": [
        {
            "name": "header",
            "area": "header",
            "title": "Header"
        },
        {
            "name": "footer",
            "area": "footer",
            "title": "Footer"
        }
